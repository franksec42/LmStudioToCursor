{
  "lm_studio": {
    "url": "http://127.0.0.1:1234",
    "timeout": 300,
    "default_model": "lm-studio-model"
  },
  "server": {
    "host": "0.0.0.0",
    "port": 8000,
    "reload": true
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  },
  "openai_compatibility": {
    "default_temperature": 0.7,
    "default_max_tokens": null,
    "default_top_p": 1.0,
    "stream_enabled": true
  }
}
